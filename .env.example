DEEPSEEK_KEY=

# Amazon Bedrock
AMAZON_KEY=
AMAZON_REGION=us-west-2
AMAZON_ACCESS_KEY_ID=
AMAZON_SECRET_ACCESS_KEY=

# Azure OpenAI
AZURE_KEY=
AZURE_ENDPOINT=
AZURE_DEPLOYMENT=

