# Discura LLM Provider API Keys
# ---------------------------------------------
# These environment variables are used to configure the LLM providers
# DO NOT commit actual API keys to version control!

# Core Configuration
PORT=3001
NODE_ENV=development
JWT_SECRET=
FRONTEND_URL=http://localhost:5173

# Database Configuration
DATABASE_URL=sqlite:./data/discura.db

# OpenRouter Configuration
USE_OPENROUTER=true
OPENROUTER_KEY=
OPENROUTER_REFERER=https://discura.ai
OPENROUTER_CACHE_TTL=43200000  # 12 hours in milliseconds

# General Caching Settings
MODEL_CACHE_TTL=86400000  # 24 hours in milliseconds
CACHE_RETRY_DELAY=60000  # 1 minute in milliseconds

# OpenAI
OPENAI_KEY=

# Anthropic
ANTHROPIC_KEY=

# Google
GOOGLE_KEY=

# Mistral
MISTRAL_KEY=

# Groq
GROQ_KEY=

