{
  "name": "@discura/common",
  "version": "1.0.0",
  "description": "Shared types and utilities for Discura",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js",
      "require": "./dist/constants.js"
    },
    "./schema/types": {
      "types": "./dist/schema/types.d.ts",
      "import": "./dist/schema/types.js",
      "require": "./dist/schema/types.js"
    },
    "./types/auth": {
      "types": "./dist/types/auth.d.ts",
      "import": "./dist/types/auth.js",
      "require": "./dist/types/auth.js"
    },
    "./auth": {
      "types": "./dist/auth/authentication.d.ts",
      "import": "./dist/auth/authentication.js",
      "require": "./dist/auth/authentication.js"
    },
    "./auth/authentication": {
      "types": "./dist/auth/authentication.d.ts",
      "import": "./dist/auth/authentication.js",
      "require": "./dist/auth/authentication.js"
    },
    "./controllers": {
      "types": "./dist/controllers/index.d.ts",
      "import": "./dist/controllers/index.js",
      "require": "./dist/controllers/index.js"
    },
    "./routes": {
      "types": "./dist/routes/routes.d.ts",
      "import": "./dist/routes/routes.js",
      "require": "./dist/routes/routes.js"
    }
  },
  "scripts": {
    "build": "npx tsc --build --force",
    "dev": "npm run build && tsc --watch",
    "clean": "rimraf dist src/schema src/routes",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.15.16",
    "@types/yamljs": "^0.2.34",
    "glob": "^11.0.2",
    "rimraf": "^5.0.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@types/express": "^5.0.1",
    "dotenv": "^16.5.0",
    "tsoa": "^6.6.0",
    "yamljs": "^0.3.0",
    "zod": "^3.24.4"
  },
  "files": [
    "dist",
    "src/schema",
    "src/routes"
  ]
}
